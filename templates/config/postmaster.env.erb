RABBITMQ_USERNAME=<%= @config['rabbitmq']['user'] %>
RABBITMQ_PASSWORD=<%= @config['rabbitmq']['password'] %>

SENDER_NAME=<%= @config['app']['name'] %>
SENDER_EMAIL=noreply@<%= @config['app']['domain'] %>

